{
    "sheet": {
        "Filename": {
            "@attr": {
                "default": "gfx/misc/default.png",
                "type": "filename"
            }
        },
        "Frames": {
            "@attr": {
                "default": 1,
                "max": 512,
                "min": 1,
                "type": "integer"
            }
        },
        "Rate": {
            "@attr": {
                "default": 0.1,
                "max": 10.0,
                "min": 0.001,
                "type": "real"
            }
        }
    }
}
